{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": false,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "insertPragma": false,
  "requirePragma": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "endOfLine": "auto",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/ui/",
    "/hooks/",
    "/screens/",
    "/components/",
    "^../(.*)",
    "^./(.*)",
    "/module.scss/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
